{
  "name": "@acme/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "echo 'Next app build is turned off for now'",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "echo 'Next app dev is turned off for now'",
    "lint": "echo 'Next app lint is turned off for now'",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "type-check": "echo 'Next app type-check is turned off for now'",
    "build:prime": "pnpm with-env next build",
    "dev:prime": "pnpm with-env next dev",
    "lint:prime": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "type-check:prime": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@acme/api": "*",
    "@acme/auth": "*",
    "@acme/db": "*",
    "@acme/tailwind-config": "*",
    "@tanstack/react-query": "^4.28.0",
    "@trpc/client": "^10.18.0",
    "@trpc/next": "^10.18.0",
    "@trpc/react-query": "^10.18.0",
    "@trpc/server": "^10.18.0",
    "next": "^13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@acme/eslint-config": "*",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2"
  }
}
